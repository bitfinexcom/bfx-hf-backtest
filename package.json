  "engines": {
    "node": ">=6"
  },
  "directories": {
    "lib": "lib"
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
