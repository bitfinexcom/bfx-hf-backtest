  "dependencies": {
    "bfx-api-node-models": "^1.0.12",
    "bfx-hf-strategy": "^1.0.5",
    "bfx-hf-util": "^1.0.11",
    "cli-table": "^0.3.1",
    "colors": "^1.3.1",
    "debug": "^4.1.1",
    "lodash": "^4.17.10",
    "p-iteration": "^1.1.7",
    "sprintf-js": "^1.1.2",
    "ws": "^6.0.0"
