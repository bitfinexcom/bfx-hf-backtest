{
  "name": "bfx-hf-backtest",
  "version": "1.0.6",
  "description": "HF backtesting logic module",
  "main": "index.js",
  "engines": {
    "node": ">=8.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "author": "Bitfinex",
  "contributors": [
    "Cris Mihalache <cris.m@bitfinex.com> (https://www.bitfinex.com)",
    "Paolo Ardoino <paolo@bitfinex.com> (https://www.bitfinex.com)",
    "Jacob Plaster <jacob.plaster@bitfinex.com> (https://www.bitfinex.com)",
    "Anton Nazarenko <anton@code-care.pro>"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "lint": "eslint examples/ test/ lib/ index.js",
    "test": "npm run lint && npm run unit",
    "unit": "NODE_ENV=test NODE_PATH=lib/ nyc --check-coverage --lines 90 --branches 60 --functions 90 --statements 90 --reporter=lcov --reporter=html mocha -b --recursive",
    "test-without-coverage": "NODE_ENV=test mocha -R spec -b --recursive",
    "build": "babel -q ./index.js -d ./dist && babel -q ./lib -d ./dist/lib && copy package.json dist",
    "postinstall": "npm run build",
    "exec_docs": "node_modules/jsdoc-to-markdown/bin/cli.js lib/*.js > docs/exec.md",
    "docs": "rm -rf docs && node_modules/.bin/jsdoc --configure .jsdoc.json --verbose"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitfinexcom/bfx-hf-backtest.git"
  },
  "bugs": {
    "url": "https://github.com/bitfinexcom/bfx-hf-backtest/issues"
  },
  "keywords": [
    "honey framework",
    "bitfinex",
    "bitcoin",
    "BTC"
  ],
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bfx-api-node-models": "^1.0.12",
    "bfx-hf-strategy": "^1.0.0",
    "bfx-hf-util": "^1.0.1",
    "cli-table": "^0.3.1",
    "colors": "^1.3.1",
    "copy": "^0.3.2",
    "debug": "^4.1.1",
    "lodash": "^4.17.10",
    "p-iteration": "^1.1.7",
    "sprintf-js": "^1.1.2",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.2.11",
    "chai": "^4.2.0",
    "compression-webpack-plugin": "^3.1.0",
    "create-index": "^2.3.0",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-lodash": "^6.0.0",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "jsdoc-to-markdown": "^5.0.1",
    "mocha": "^7.1.0",
    "nyc": "^15.0.0",
    "standard": "^14.2.0",
    "webpack": "^4.42.0"
  }
}
