    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.0.1",
    "hyperbee": "0.0.16",
    "hypercore": "^9.5.0",
    "istanbul": "^0.4.5",
    "jsdoc-to-markdown": "^5.0.1",
    "mocha": "^6.2.0",
    "standard": "^14.2.0"
  }
